{
  "name": "mixtape_back",
  "version": "1.0.0",
  "description": "mixtape-Back",
  "main": "bin/www",
  "scripts": {
    "start": "NODE_ENV=development DEBUG=mixtape CONFIG=development nodemon ./bin/www",
    "start:devDocker": "NODE_ENV=development node bin/www",
    "start:testing": "DEBUG=* CONFIG=testing nodemon ./bin/www",
    "pm2_test": "NODE_ENV=testing DEBUG=* CONFIG=testing pm2 start ./bin/www --name mixtape_8001",
    "pm2_prod": "NODE_ENV=production CONFIG=production pm2 start ./bin/www --name mixtape_8001",
    "pm2_delete_app": "pm2 delete mixtape_8001 && pm2 delete socket_server_8002",
    "start:local": "CONFIG=development_local nodemon bin/www",
    "socket": "node socket/index.js",
    "socket_pm2": "pm2 start socket/index.js --name socket_server_8002",
    "lint": "eslint . --quiet",
    "swagger": "apidoc-swagger -i app -o public/swagger",
    "apidoc": "apidoc -i app -o public/doc",
    "test": "NODE_ENV=test mocha --timeout 10000",
    "token": "node ./scripts/generate-user-token.js",
    "seed:users": "DEBUG=* node cli/seeds/users.js seed:users",
    "postinstall": "node -e \"var s=['../src','../test'],d=['node_modules/app','node_modules/test'],fs=require('fs');s.forEach(function(_,i){fs.existsSync(d[i])||fs.symlinkSync(s[i],d[i],'dir')});\"",
    "agenda": "NODE_ENV=development DEBUG=lambda CONFIG=development nodemon --harmony ./worker/start.js",
    "agenda:prod": "NODE_ENV=production pm2 start ./worker/start.js --name worker_agenda"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/btomashvili/mixtape-Back.git"
  },
  "author": "Beka Tomashvili, Giga Chkhikvadze",
  "license": "ISC",
  "homepage": "https://github.com/btomashvili/mixtape-Back",
  "config": {
    "unsafe-perm": true
  },
  "dependencies": {
    "@google-cloud/logging": "^4.1.0",
    "@sendgrid/mail": "^6.1.4",
    "agenda": "2.0.1",
    "apollo-upload-server": "5.0.0",
    "axios": "^0.18.0",
    "bcrypt-nodejs": "0.0.3",
    "bluebird": "^3.4.7",
